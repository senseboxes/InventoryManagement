<div class="container">
	<h1 class="bg-info" align="center">생산품명 등록</h1>

		<%= form_tag(action: :productnameset_write_complete) do %>
		<div align="center" class="field">
			<label>분류 명</label>
				<%= collection_select :category, :category_id, Category.all , :id, :name %>
				<div align="center" class="field">
					<label>생산품명</label>
					<input name="productname" id="productnameset" size="20" type="text"/>
					<label>설명</label>
					<input name="description" id="productnameset" size="40" type="text"/>
				</div>
				<div align="center" class="actions">
					<button type="submit" class="btn btn-default">저 장</button> | <a href="/productnameset"><button  type="button" class="btn btn-default">뒤로가기</button></a>
				</div>
				<% end %>
		</div>
</div>
