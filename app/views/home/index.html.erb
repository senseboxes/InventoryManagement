<div align="center" class="container">
  <%= form_tag do %>
    <h2>분류 선택</h2>
    <% @category_list.each do |list| %>
      <label><a href="/index_category/<%= list.id %>"><button type="submit" class="btn btn-default" name="cookie_name" value="<%= list.id %>"><%= list.name %></button></a></label>
    <% end %>
  <% end %>
</div>
